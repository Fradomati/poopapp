{
    "version": 2,
    "name": "Pooptime",
    "public": false,
    "builds": [
        {
            "src": "/client/package.json",
            "use": "@now/static-build"
        },
        {
            "src": "/back/bin/www",
            "use": "@now/node-server"
        }
    ],
    "routes": [
        {
            "src": "/back/(.*)",
            "dest": "/back/$1"
        },
        {
            "src": "/client/(.*)",
            "dest": "/client/$1"
        }
    ]
}